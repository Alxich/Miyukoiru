{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": ["es6"],
		"allowJs": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"include": ["./**/*"],
	"exclude": [
		"./bin/**",
		"./dist/**/*" // This is what fixed it!
	]
}